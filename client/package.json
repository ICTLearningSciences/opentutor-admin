{
  "name": "opentutor-admin",
  "description": "web ui for grading opentutor sessions",
  "version": "1.0.0",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@reach/router": "^1.3.4",
    "axios": "^0.21.1",
    "gatsby": "^2.32.3",
    "jsoneditor": "^9.1.2",
    "jsoneditor-react": "^3.0.1",
    "react": "^17.0.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-cookie": "^4.0.3",
    "react-dom": "^17.0.1",
    "react-google-login": "^5.1.22",
    "react-toastify": "^6.1.0",
    "universal-cookie": "^4.0.4",
    "url-join": "^4.0.1",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.56",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-dom": "^16.9.9",
    "@types/url-join": "^4.0.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby-cli": "^2.19.1",
    "gatsby-plugin-env-variables": "^2.0.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-manifest": "^2.6.0",
    "gatsby-plugin-material-ui": "^2.1.10",
    "gatsby-plugin-typescript": "^2.6.0",
    "license-check-and-add": "^3.0.4",
    "npm-check-updates": "^10.0.0",
    "prettier": "^2.1.2",
    "typescript": "^4.0.5"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npx gatsby build --prefix-paths",
    "dependencies-update": "npx ncu -u",
    "develop": "npx gatsby develop",
    "format": "prettier --write '**/*.{js,jsx,json,ts,tsx}'",
    "start": "npm run develop",
    "serve": "gatsby serve --prefix-paths",
    "license:fix": "license-check-and-add remove -f license-config.json && license-check-and-add add -f license-config.json",
    "test:license": "license-check-and-add check -f license-config.json",
    "test": "echo 'all tests are cypress for now'",
    "test:all": "npx run test:format && npx run test:lint && npx run test:audit && npx run test:types && npx run test:cypress && npx test",
    "test:audit": "npm audit --level=moderate",
    "test:cypress": "npx run cy:run --spec 'cypress/**/*.spec.js'",
    "test:format": "prettier --check '**/*.{js,jsx,json,ts,tsx}'",
    "test:lint": "eslint src --ext ts --ext tsx --ext js --ext jsx",
    "test:types": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ictlearningsciences/opentutor-admin.git"
  },
  "bugs": {
    "url": "https://github.com/ictlearningsciences/opentutor-admin/issues"
  }
}
