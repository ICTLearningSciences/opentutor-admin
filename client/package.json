{
  "name": "opentutor-admin",
  "description": "web ui for grading opentutor sessions",
  "version": "1.0.0",
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@reach/router": "^1.3.4",
    "axios": "^0.21.1",
    "gatsby": "^3.5.0",
    "jsoneditor": "^9.4.1",
    "jsoneditor-react": "^3.1.1",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-cookie": "^4.0.3",
    "react-dom": "^17.0.2",
    "react-google-login": "^5.2.2",
    "react-toastify": "^7.0.4",
    "universal-cookie": "^4.0.4",
    "url-join": "^4.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^15.0.3",
    "@types/react": "^17.0.5",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-dom": "^17.0.5",
    "@types/url-join": "^4.0.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby-plugin-env-variables": "^2.1.0",
    "gatsby-plugin-eslint": "^3.0.0",
    "gatsby-plugin-manifest": "^3.5.0",
    "gatsby-plugin-material-ui": "^3.0.1",
    "gatsby-plugin-typescript": "^3.5.0",
    "npm-check-updates": "^11.5.11",
    "prettier": "^2.3.0",
    "typescript": "^4.2.4"
  },
  "keywords": [
    "gatsby"
  ],
  "resolutions": {
    "trim": "1.0.1"
  },
  "scripts": {
    "build": "npx gatsby build --prefix-paths",
    "dependencies-update": "npx ncu -u",
    "develop": "npx gatsby develop",
    "format": "prettier --write '**/*.{js,jsx,json,ts,tsx}'",
    "preinstall": "npx npm-force-resolutions",
    "start": "npm run develop",
    "serve": "gatsby serve --prefix-paths",
    "test": "echo 'all tests are cypress for now'",
    "test:all": "npx run test:format && npx run test:lint && npx run test:audit && npx run test:types && npx run test:cypress && npx test",
    "test:audit": "npm audit --level=moderate --production",
    "test:cypress": "npx run cy:run --spec 'cypress/**/*.spec.js'",
    "test:format": "prettier --check '**/*.{js,jsx,json,ts,tsx}'",
    "test:lint": "eslint src --ext ts --ext tsx --ext js --ext jsx --max-warnings=0",
    "test:types": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ictlearningsciences/opentutor-admin.git"
  },
  "bugs": {
    "url": "https://github.com/ictlearningsciences/opentutor-admin/issues"
  }
}
